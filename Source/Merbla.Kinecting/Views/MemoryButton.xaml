<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:Merbla_Kinecting_WPF_Controls="clr-namespace:Merbla.Kinecting.WPF.Controls;assembly=Merbla.Kinecting.WPF.Controls"
	mc:Ignorable="d"
	x:Class="Merbla.Kinecting.MemoryButton"
	x:Name="UserControl"
	d:DesignWidth="100" d:DesignHeight="100">

	<Grid x:Name="LayoutRoot">
		<Merbla_Kinecting_WPF_Controls:ContentControl3D>
			<Merbla_Kinecting_WPF_Controls:ContentControl3D.BackContentTemplate>
				<DataTemplate>
					<Button Content="Go Back" Command="Merbla_Kinecting_WPF_Controls:ContentControl3D.RotateCommand" />
				</DataTemplate>
			</Merbla_Kinecting_WPF_Controls:ContentControl3D.BackContentTemplate>
			<Button Content="Test" Command="Merbla_Kinecting_WPF_Controls:ContentControl3D.RotateCommand" />
		</Merbla_Kinecting_WPF_Controls:ContentControl3D>
	</Grid>
</UserControl>